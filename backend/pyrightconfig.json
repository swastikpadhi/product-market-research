{
  "venvPath": "../..",
  "venv": "venv",
  "extraPaths": [
    ".",
    "./backend"
  ],
  "pythonVersion": "3.13",
  "typeCheckingMode": "basic",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "reportOptionalMemberAccess": false,
  "reportGeneralTypeIssues": false,
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "venv",
    "frontend"
  ]
}
